<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="RedBus Regression Filter Suite">
    <listeners>
        <listener class-name="com.redbus.test.TestComponents.Listeners"/>
    </listeners>

    <test name="Regression Filter Tests">
        <groups>
            <run>
                <include name="regression_filter"/>
            </run>
        </groups>

        <classes>
            <class name="com.redbus.test.E2E_Test"/>
        </classes>
    </test>
</suite>
